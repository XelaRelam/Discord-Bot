{
  "name": "xelarelam_bot",
  "version": "1.4.0",
  "description": "Bot For XelaRelam",
  "author": "LynnuxDev",
  "license": "BSD-4-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/XelaRelam/Discord-Bot"
  },
  "main": "dist/index.js",
  "scripts": {
    "deploy:commands": "ts-node scripts/deployCommands.ts",
    "lint": "eslint '**/*.{js,ts,tsx}' --cache && git add . && git commit -m 'style: lint'",
    "release": "npx commit-and-tag-version",
    "build": "tsc",
    "start": "node dist/bot/index.js",
    "dev": "nodemon --watch src --ext ts --exec ts-node -r src/bot/index.ts",
    "test": "npm run build && node dist/tests/hello",
    "db": "npx prisma format && npx prisma generate && npx prisma db push"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "keywords": [],
  "packageManager": "pnpm@10.6.5",
  "dependencies": {
    "@discordjs/builders": "^1.10.1",
    "@discordjs/rest": "^2.4.3",
    "@prisma/client": "^6.5.0",
    "axios": "^1.7.9",
    "discord-api-types": "^0.37.119",
    "discord.js": "^14.18.0",
    "dotenv": "^16.4.7",
    "nodemon": "^3.1.9",
    "path": "^0.12.7",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "commit-and-tag-version": "^12.5.0",
    "eslint": "^9.17.0",
    "eslint-define-config": "^2.1.0",
    "pidusage": "^4.0.0",
    "winston": "^3.17.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "esbuild",
      "prisma"
    ]
  }
}
